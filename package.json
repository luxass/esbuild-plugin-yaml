{
  "name": "@luxass/esbuild-yaml",
  "version": "0.1.0",
  "description": "An esbuild plugin, that allows imports of YAML files",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@8.12.0",
  "license": "MIT",
  "homepage": "https://github.com/luxass/esbuild-yaml",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/esbuild-yaml"
  },
  "bugs": {
    "url": "https://github.com/luxass/esbuild-yaml/issues"
  },
  "keywords": [
    "esbuild",
    "plugin",
    "yaml",
    "yaml import",
    "yaml plugim",
    "yaml esbuild"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "yaml.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --report-unused-disable-directives",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "esbuild": ">=0.18"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.0.0-beta.16",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.4",
    "esbuild": "^0.19.0",
    "eslint": "^8.55.0",
    "eslint-plugin-format": "^0.1.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.2"
  }
}
