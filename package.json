{
  "name": "@zotera/esbuild-yaml",
  "version": "0.1.0",
  "description": "A esbuild plugin, that allows imports of YAML files",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write --cache .",
    "lint": "eslint --cache {src,test}/**/*.ts",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run"
  },
  "repository": "https://github.com/zotera/esbuild-yaml",
  "keywords": [
    "esbuild",
    "plugin",
    "yaml",
    "yaml import",
    "yaml plugim",
    "yaml esbuild"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zotera/esbuild-yaml/issues"
  },
  "homepage": "https://github.com/zotera/esbuild-yaml#readme",
  "author": {
    "name": "Lucas Norgaard",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^1.0.6",
    "@luxass/prettier-config": "^1.0.4",
    "@types/node": "^18.0.3",
    "esbuild": "^0.14.49",
    "eslint": "^8.20.0",
    "prettier": "^2.7.1",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vitest": "^0.18.1"
  },
  "packageManager": "pnpm@7.5.0"
}
